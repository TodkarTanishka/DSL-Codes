#include<iostream>
using namespace std;
int adj[5][5];
int visited[5];

void DFS(int v, int n){
	visited[v] = 1;
	cout<<v<<" ";
	for(int i = 0; i<n; i++)
	{
		if(adj[v][i]==1 && visited[i]==0)
		{
			DFS(i,n);
		}
	}
}

int main()
{
	int n, e, u, v;
	cout<<"Enter number of vertices:";
	cin>>n;
	cout<<"Enter number of edges:";
	cin>>e;
	for(int i = 0; i<n; i++)
	{
		for(int j=0;j<n; j++)
		{
			adj[i][j] = 0;
		}
		visited[i] = 0;
	}
	
	cout<<"Enter the Edges:\n";
	for(int k = 0; k<e; k++)
	{
		cin>>u>>v;
		adj[u][v] = 1;
		adj[v][u] = 1;
	}
	
	cout<<"Adjacency Matrix:\n";
	for(int i = 0; i<n; i++)
	{
		for(int j=0; j<n; j++)
		{
			cout<<adj[i][j]<<"\t";
		}
		cout<<"\n";
	}
	
	cout<<"\nDFS Display:";
	DFS(0, n);
	return 0;
}
